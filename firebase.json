{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
        {
            "source": "**",
            "destination": "/index.html"
        }
    ]
  },
  "emulators": {
    "hosting": {
      "host": "127.0.0.1",
      "port": "5001"
    }
  }
}
